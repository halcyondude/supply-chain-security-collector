{
  "name": "github-repo-supply-chain-datacollector",
  "version": "1.0.0",
  "description": "Generic GraphQL data collection tool with DuckDB storage and Parquet export",
  "scripts": {
    "test": "npm run test:three",
    "start": "npm run landscape",
    "landscape": "ts-node src/neo.ts --input input/cncf-full-landscape.json --queries GetRepoDataExtendedInfo --analyze --parallel",
    "test:single": "ts-node src/neo.ts --input input/test-single-project.json --queries GetRepoDataExtendedInfo --analyze --parallel",
    "test:three": "ts-node src/neo.ts --input input/test-three-projects.json --queries GetRepoDataExtendedInfo --analyze --parallel",
    "test:simple": "ts-node src/neo.ts --input input/test-simple-format.json --queries GetRepoDataExtendedInfo --analyze --parallel",
    "collect": "ts-node src/neo.ts",
    "analyze": "ts-node src/analyze.ts",
    "fetch:landscape": "ts-node scripts/fetch-cncf-landscape.ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "postinstall": "npm run codegen",
    "lint": "npx eslint src/",
    "lint:fix": "npx eslint src/ --fix",
    "format": "npx eslint src/ --fix",
    "typecheck": "npx tsc --noEmit",
    "clean": "rm -rf output .cache src/generated"
  },
  "keywords": [
    "graphql",
    "duckdb",
    "parquet",
    "data-engineering",
    "etl"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@duckdb/node-api": "^1.4.1-r.1",
    "apache-arrow": "^21.1.0",
    "chalk": "4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "dotenv": "^16.3.1",
    "duckdb": "^1.1.3",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "node-cache": "^5.1.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.0",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@types/node": "^20.8.9",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
